#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 1;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1e2ebb0 .scope module, "sort_test" "sort_test" 2 3;
 .timescale 0 -1;
v0x1e58910_0 .var "clk", 0 0;
v0x1e589b0_0 .var "key", 1 0;
v0x1e58a30_0 .net "led", 7 0, v0x1e58280_0; 1 drivers
v0x1e58ae0_0 .var "sw", 3 0;
v0x1e58bc0_0 .var "tempdata", 7 0;
L_0x1e58c40 .part v0x1e589b0_0, 0, 1;
L_0x1e58d40 .part v0x1e589b0_0, 1, 1;
S_0x1e586c0 .scope task, "insert" "insert" 2 32, 2 32, S_0x1e2ebb0;
 .timescale 0 -1;
v0x1e587b0_0 .var "data", 3 0;
v0x1e58870_0 .var "out_data", 7 0;
TD_sort_test.insert ;
    %vpi_call 2 36 "$display", "Inserted %b", v0x1e587b0_0;
    %load/v 8, v0x1e587b0_0, 4;
    %set/v v0x1e58ae0_0, 8, 4;
    %ix/load 0, 0, 0;
    %set/x0 v0x1e589b0_0, 1, 1;
    %wait E_0x1e08600;
    %delay 10, 0;
    %ix/load 0, 0, 0;
    %set/x0 v0x1e589b0_0, 0, 1;
    %load/v 8, v0x1e58a30_0, 8;
    %set/v v0x1e58870_0, 8, 8;
    %vpi_call 2 44 "$display", "--Found %b", v0x1e58870_0;
    %end;
S_0x1e58510 .scope task, "run_sort" "run_sort" 2 48, 2 48, S_0x1e2ebb0;
 .timescale 0 -1;
v0x1e58600_0 .var "out_data", 7 0;
TD_sort_test.run_sort ;
    %ix/load 0, 1, 0;
    %set/x0 v0x1e589b0_0, 1, 1;
    %wait E_0x1e08600;
    %delay 10, 0;
    %ix/load 0, 1, 0;
    %set/x0 v0x1e589b0_0, 0, 1;
    %load/v 8, v0x1e58a30_0, 8;
    %set/v v0x1e58600_0, 8, 8;
    %vpi_call 2 57 "$display", "--Found %b", v0x1e58600_0;
    %end;
S_0x1e2e860 .scope module, "ss" "sort" 2 10, 3 1, S_0x1e2ebb0;
 .timescale 0 0;
v0x1e093b0 .array "array", 3 0, 1 0;
v0x1e1af70_0 .net "clk", 0 0, v0x1e58910_0; 1 drivers
v0x1e57f10_0 .var/i "i", 31 0;
v0x1e57fb0_0 .var/i "index", 31 0;
v0x1e58060_0 .var/i "j", 31 0;
v0x1e58100_0 .net "key1", 0 0, L_0x1e58c40; 1 drivers
v0x1e581e0_0 .net "key2", 0 0, L_0x1e58d40; 1 drivers
v0x1e58280_0 .var "led", 7 0;
v0x1e58370_0 .net "sw", 3 0, v0x1e58ae0_0; 1 drivers
v0x1e58410_0 .var "temp", 1 0;
E_0x1e08600 .event posedge, v0x1e1af70_0;
    .scope S_0x1e2e860;
T_2 ;
    %wait E_0x1e08600;
    %load/v 8, v0x1e58100_0, 1;
    %jmp/0xz  T_2.0, 8;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.2, 4;
    %load/x1p 40, v0x1e58370_0, 2;
    %jmp T_2.3;
T_2.2 ;
    %mov 40, 2, 2;
T_2.3 ;
    %mov 8, 40, 2; Move signal select into place
    %mov 10, 0, 30;
    %set/v v0x1e57fb0_0, 8, 32;
    %load/v 8, v0x1e58370_0, 2; Only need 2 of 4 bits
; Save base=8 wid=2 in lookaside.
    %ix/getv/s 3, v0x1e57fb0_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x1e093b0, 8, 2;
t_0 ;
T_2.0 ;
    %load/v 8, v0x1e581e0_0, 1;
    %jmp/0xz  T_2.4, 8;
    %movi 8, 3, 32;
    %set/v v0x1e57f10_0, 8, 32;
T_2.6 ;
    %load/v 8, v0x1e57f10_0, 32;
    %cmp/s 0, 8, 32;
    %or 5, 4, 1;
    %jmp/0xz T_2.7, 5;
    %set/v v0x1e58060_0, 0, 32;
T_2.8 ;
    %load/v 8, v0x1e58060_0, 32;
    %load/v 40, v0x1e57f10_0, 32;
    %cmp/s 8, 40, 32;
    %jmp/0xz T_2.9, 5;
    %ix/load 0, 1, 0;
    %load/vp0/s 10, v0x1e58060_0, 32;
    %ix/get/s 3, 10, 32;
    %load/av 8, v0x1e093b0, 2;
    %ix/getv/s 3, v0x1e58060_0;
    %load/av 10, v0x1e093b0, 2;
    %cmp/u 8, 10, 2;
    %jmp/0xz  T_2.10, 5;
    %ix/getv/s 3, v0x1e58060_0;
    %load/av 8, v0x1e093b0, 2;
    %set/v v0x1e58410_0, 8, 2;
    %ix/load 0, 1, 0;
    %load/vp0/s 10, v0x1e58060_0, 32;
    %ix/get/s 3, 10, 32;
    %load/av 8, v0x1e093b0, 2;
    %ix/getv/s 3, v0x1e58060_0;
   %jmp/1 t_1, 4;
   %ix/load 1, 0, 0;
   %set/av v0x1e093b0, 8, 2;
t_1 ;
    %load/v 8, v0x1e58410_0, 2;
    %ix/load 0, 1, 0;
    %load/vp0/s 10, v0x1e58060_0, 32;
    %ix/get/s 3, 10, 32;
   %jmp/1 t_2, 4;
   %ix/load 1, 0, 0;
   %set/av v0x1e093b0, 8, 2;
t_2 ;
T_2.10 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x1e58060_0, 32;
    %set/v v0x1e58060_0, 8, 32;
    %jmp T_2.8;
T_2.9 ;
    %load/v 8, v0x1e57f10_0, 32;
    %subi 8, 1, 32;
    %set/v v0x1e57f10_0, 8, 32;
    %jmp T_2.6;
T_2.7 ;
T_2.4 ;
    %movi 8, 3, 32;
    %set/v v0x1e57f10_0, 8, 32;
T_2.12 ;
    %load/v 8, v0x1e57f10_0, 32;
    %cmp/s 0, 8, 32;
    %or 5, 4, 1;
    %jmp/0xz T_2.13, 5;
    %movi 8, 7, 33;
    %load/v 41, v0x1e57f10_0, 32;
    %mov 73, 72, 1;
    %muli 41, 2, 33;
    %sub 8, 41, 33;
    %set/v v0x1e57fb0_0, 8, 32;
    %ix/getv/s 3, v0x1e57f10_0;
    %load/av 8, v0x1e093b0, 2;
    %load/v 10, v0x1e57fb0_0, 32;
    %mov 42, 41, 1;
    %subi 10, 1, 33;
    %ix/get/s 0, 10, 33;
    %jmp/1 t_3, 4;
    %set/x0 v0x1e58280_0, 8, 2;
t_3 ;
    %load/v 8, v0x1e57f10_0, 32;
    %subi 8, 1, 32;
    %set/v v0x1e57f10_0, 8, 32;
    %jmp T_2.12;
T_2.13 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x1e2ebb0;
T_3 ;
    %set/v v0x1e58910_0, 0, 1;
    %set/v v0x1e587b0_0, 1, 4;
    %fork TD_sort_test.insert, S_0x1e586c0;
    %join;
    %load/v 8, v0x1e58870_0, 8;
    %set/v v0x1e58bc0_0, 8, 8;
    %movi 8, 9, 4;
    %set/v v0x1e587b0_0, 8, 4;
    %fork TD_sort_test.insert, S_0x1e586c0;
    %join;
    %load/v 8, v0x1e58870_0, 8;
    %set/v v0x1e58bc0_0, 8, 8;
    %movi 8, 6, 4;
    %set/v v0x1e587b0_0, 8, 4;
    %fork TD_sort_test.insert, S_0x1e586c0;
    %join;
    %load/v 8, v0x1e58870_0, 8;
    %set/v v0x1e58bc0_0, 8, 8;
    %movi 8, 3, 4;
    %set/v v0x1e587b0_0, 8, 4;
    %fork TD_sort_test.insert, S_0x1e586c0;
    %join;
    %load/v 8, v0x1e58870_0, 8;
    %set/v v0x1e58bc0_0, 8, 8;
    %vpi_call 2 23 "$display", "Before sort %b", v0x1e58bc0_0;
    %fork TD_sort_test.run_sort, S_0x1e58510;
    %join;
    %load/v 8, v0x1e58600_0, 8;
    %set/v v0x1e58bc0_0, 8, 8;
    %vpi_call 2 25 "$display", "After sort %b", v0x1e58bc0_0;
    %end;
    .thread T_3;
    .scope S_0x1e2ebb0;
T_4 ;
    %delay 50, 0;
    %load/v 8, v0x1e58910_0, 1;
    %inv 8, 1;
    %set/v v0x1e58910_0, 8, 1;
    %jmp T_4;
    .thread T_4;
